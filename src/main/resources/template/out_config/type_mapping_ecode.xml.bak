<?xml version="1.0" encoding="UTF-8"?>
<mapping-config>
	<tran id="${ecode}$" channel="${systemAb}$">
			<request-mapping base="default" dict="${systemAb}$">
			    <request-head channel="${systemAb}$"/>				
				${request}$
			</request-mapping>
			<response-mapping base="default" dict="esb">
				<response-head channel="${systemAb}$"/>
				${response}$
			</response-mapping>
			
			<error-mapping base="default" dict="esb"> 
				<error-head channel="${systemAb}$"/>			
			</error-mapping>
	</tran>
</mapping-config>